{
  "AWSTemplateFormatVersion": "2010-09-09",
  "Resources": {
    "testbucket1": {
      "Type": "AWS::S3::Bucket",
      "Properties": {
        "BucketName": "testbucket1",
        "AccessControl": "Private",
        "VersioningConfiguration": {
          "Status": "Suspended"
        }
      },
      "DeletionPolicy": "Retain"
    },
    "testbucket2": {
      "Type": "AWS::S3::Bucket",
      "Properties": {
        "BucketName": "testbucket2",
        "AccessControl": "Private",
        "VersioningConfiguration": {
          "Status": "Enabled"
        }
      },
      "DeletionPolicy": "Retain"
    }
  },
  "Description": "Demo CloudFormation template."
}